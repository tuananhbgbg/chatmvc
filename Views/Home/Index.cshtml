@{ Layout = "_Layout";}
@using ChatMVC
@model ChannelChat
@Html.Partial("_nav")

<div class="messaging">
    <div class="inbox_msg">
        <div class="inbox_people">
            <div class="headind_srch">
                <div class="recent_heading">
                    <h4>Recent</h4>
                </div>
                <div class="srch_bar">
                    <div class="stylish-input-group">
                        <input type="text" class="search-bar" placeholder="Search">
                        <span class="input-group-addon">
                            <button type="button"> <i class="fa fa-search" aria-hidden="true"></i> </button>
                        </span>
                    </div>
                </div>
            </div>
            @*Channel box*@
            <div class="inbox_chat">
                <div class="chat_list active_chat">
                    <div class="chat_people">
                        <div class="chat_img"> <img src="data:image/png;base64," alt="General"> </div>
                        <div class="chat_ib">
                            <h5>General Channel <span class="chat_date"></span></h5>
                            <p>
                                All green foxes are here
                            </p>
                        </div>
                    </div>
                </div>
                
            </div>
        </div>
        <div class="mesgs">
            <div class="msg_history" id="msgBox">
                @foreach (var message in Model.Messages)
                {
                    <div class="incoming_msg">
                        <div class="incoming_msg_img" data-toggle="tooltip" data-placement="left" title=@message.Author?.UserName>
                                <img src=@message.Author?.AvatarUrl alt=>
                        </div>
                        <div class="received_msg">
                            <div class="received_withd_msg">
                                <p>@message.Content</p>
                                <span class="time_date">@message.Created.ToString("t")</span>
                          
                            </div>
                        </div>
                    </div>
                }
            </div>
            <div class="type_msg">
                <div class="input_msg_write">
                    <form action="/message" method="post">
                        <input type="text" class="write_msg" placeholder="Type a message" name="content" />
                        <button type="submit" class="msg_send_btn" ><i class="fa fa-paper-plane-o" aria-hidden="true"></i></button>
                    </form>
                </div>
            </div>
        </div>
    </div>


    <p class="text-center top_spac"> Design by <a target="_blank" href="#">Sunil Rajput</a></p>

</div>
